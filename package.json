{
  "name": "Classworks",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "start": "electron ."
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@microsoft/clarity": "^1.0.0",
    "axios": "^1.11.0",
    "idb": "^8.0.3",
    "js-base64": "^3.7.8",
    "js-yaml": "^4.1.0",
    "pinyin-pro": "^3.27.0",
    "ratelimit-header-parser": "^0.1.0",
    "roboto-fontface": "*",
    "typewriter-effect": "^2.21.0",
    "uuid": "^9.0.1",
    "vue": "^3.5.20",
    "vuetify": "^3.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.14.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.30.0",
    "pinia": "^3.0.3",
    "sass": "1.86.3",
    "sass-embedded": "^1.86.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^5.4.17",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-router": "^4.5.1"
  },
  "build": {
    "appId": "com.classworks.app",
    "productName": "Classworks",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "**/*",
      "!node_modules",
      "!dist",
      "!dist_electron"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "description": "A desktop application for ClassWork.",
  "author": "DannyFeng"
